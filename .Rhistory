add_numbers <- function(vec) {
result <- character(length(vec))
counts <- list()
for (i in seq_along(vec)) {
element <- vec[i]
if (element %in% names(counts)) {
counts[[element]] <- counts[[element]] + 1
} else {
counts[[element]] <- 1
}
result[i] <- paste(element, counts[[element]], sep = " ")
}
return(result)
}
